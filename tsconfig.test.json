{
  "include": [
    "src/data/**/*.ts",
    "src/data/*.ts",
    "src/char/**/*.ts",
    "src/char/*.ts",
    "src/data/**/*.ts",
    "src/data/*.ts",
    "src/lib/*.ts",
    "src/menu/*.ts",
    "test/unit/*.ts"
, "src/preload/AppConfigIPC.ts", "src/preload/LocalDBClientIPC.ts", "src/preload/LocalMLTClientIPC.ts", "src/preload/MarkDBClientIPC.ts"  ],
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "sourceMap": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "useDefineForClassFields": true,
    "strict": true,
    "noImplicitThis": true,
    "esModuleInterop": true,
    "allowJs": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "traceResolution": true,
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@model/*": ["src/data/model/*"],
      "@config/*": ["src/data/config/*"]
    }
  }
}
